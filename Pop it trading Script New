local kavoUi = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local window = kavoUi.CreateLib("Pop it Trading Script Made by xdkentryanborbon","DarkTheme")

---Tabs

local Tab1 = window:NewTab("Auto Buy Item")
local Tab1Section = Tab1:NewSection("Auto Buy Valentine Items")
local Tab2 = window:NewTab("Buy Item")
local Tab2Section = Tab2:NewSection("Buy Valentine Items")
local Tab3 = window:NewTab("Credits")
local Tab3Section = Tab3:NewSection("Made By xdkentryanborbon")
local Tab3Section = Tab3:NewSection("Subscribe To xdkentryanborbon For More")

---Buttons

Tab1Section:NewToggle("auto buy Heart Lightstick", "auto buys", function(state) if state then _G.on = true while _G.on do wait() local args = { [1] = "Heart Lightstick" } 
game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end end)

